        <value>// This script needs to set answer to 'yes' or 'no' to indicate the state of the activity.&#13;
//&#13;
// For example,&#13;
//&#13;
//x_227277_zombie_wo_zombie_world_table&#13;
//x_227277_zombie_wo_zombie_player&#13;
//x_227277_zombie_wo_zombie_entity&#13;
//x_227277_zombie_wo_zombie_weapons_locker&#13;
&#13;
&#13;
&#13;
var tableName = "x_227277_zombie_wo_zombie_weapons_locker";&#13;
answer = 'no';&#13;
gs.addInfoMessage('wfWL_WAL.1-- SubFlow Weapons Locker - Weapon Available - Now Running.   ');&#13;
try&#13;
{&#13;
	var tblWeaponsLocker = new GlideRecord('x_227277_zombie_wo_zombie_weapons_locker');&#13;
	if(!tblWeaponsLocker)&#13;
	{&#13;
		gs.addErrorMessage('wfWL_WAL.2-- Table name "Weapons Locker" DOES NOT EXIST !!');&#13;
	}	&#13;
&#13;
	if(tblWeaponsLocker.canRead())&#13;
	{&#13;
		gs.addInfoMessage('wfWL_WAL.3-- WeaponsLocker is READABLE !!');&#13;
	}&#13;
	else&#13;
	{&#13;
		gs.addInfoMessage('wfWL_WAL.4-- ERROR -- WeaponsLocker Not READABLE !!');&#13;
	}&#13;
	//tblWeaponsLocker.addQuery('zombie_player_alive', 'true');&#13;
	gs.addInfoMessage('wfWL_WAL.5-- Attempting to Query the "Weapons Locker Table without a CLAUSE ..."   ');&#13;
	tblWeaponsLocker.query();&#13;
	gs.addInfoMessage('wfWL_WAL.6-- Weapons Locker has been Queried.');&#13;
	var recordsFound = tblWeaponsLocker.getRowCount();&#13;
	if(recordsFound &gt; 0)&#13;
	{&#13;
		while(tblWeaponsLocker.next()) &#13;
		{&#13;
			gs.addInfoMessage('wfWL_WAL.7-- Found a Weapon available in the Locker !!');&#13;
			if(tblWeaponsLocker.zw_weapon_safety_on == 'true')&#13;
			{&#13;
			    gs.addInfoMessage('wfWL_WAL.8A-- Safety ON');&#13;
				if(tblWeaponsLocker.zw_weapon_ammo_count &lt;= 0)&#13;
				{&#13;
					answer = 'yes';  //comment&#13;
				}&#13;
				else&#13;
				{&#13;
				    gs.addInfoMessage('wfWL_WAL.8B-- Weapon ALREADY LOADED. NOT SAFE. NOT ISSUED !!');		&#13;
				}&#13;
			}&#13;
			else&#13;
			{&#13;
			    gs.addInfoMessage('wfWL_WAL.8B-- Weapon NOT SAFE. NOT ISSUED !!');&#13;
			}&#13;
		}&#13;
	}&#13;
	else&#13;
	{&#13;
		gs.addInfoMessage('wfWL_WAL.9-- NO WEAPONS FOUND IN LOCKER !!');&#13;
	}&#13;
&#13;
	&#13;
}&#13;
catch(e)&#13;
{&#13;
	gs.addErrorMessage("wfWL_WAL.10: Exception Encountered While Attempting to create a GlideRecord for Table:" + tableName);&#13;
	gs.addErrorMessage(e.message);&#13;
	gs.error(e.message);	&#13;
}&#13;
